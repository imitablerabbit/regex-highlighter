// FUNCTION TESTING
int factorial(int number) {
    if (number <= 1)
	    return 1;
    return number * factorial(number-1);
}

//OBJECT TESTING
hello.property = 3;
Hello->property = 3;
Hello->newHello->property = 3;
Hello.init();
hello->init();

